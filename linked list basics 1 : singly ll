#include <iostream>
using namespace std;

//implementation of node

class Node{
    public:
    int data;
    Node *next;

    //constructor
    Node(int data){
        this->data = data ;
        this->next = NULL;

    }

    void InsertBegin(Node* &head, int d){
        //new node crate
        Node* temp = new Node(d);
        temp->next = head ;
        head = temp ;
    }

    //traversing and printing of node
    void print(Node*& head){
        Node* temp = head;
        while(temp!= NULL ){
            cout<<temp->data<<" ";
            temp = temp->next;

        }
        cout<<endl;

    }
    void insertionend(Node* &tail, int data){
        Node* temp = new Node();
        tail->next = temp;
        tail = tail->next;
    }

};
int main()
{
    Node* node1 = new Node(10);
    // cout<<node1 ->data<<endl;
    // cout<<node1 ->next<<endl;

    Node* head = node1;
    Node* tail = node1;

    node1->print(head);
    node1->InsertBegin(head,12);
    node1->print(head); //12 10

    node1->print(tail);
    node1->InsertBegin(tail,12);
    node1->print(tail); //12 10

    
  return 0;
}
